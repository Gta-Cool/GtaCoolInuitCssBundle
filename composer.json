{
    "name": "gta-cool/inuitcss-bundle",
    "type": "symfony-bundle",
    "description": "Integrates inuit.css framework in Symfony2",
    "keywords": [
        "SASS",
        "BEM",
        "OOCSS",
        "framework"
    ],
    "homepage": "https://github.com/Gta-Cool/GtaCoolInuitCssBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Plugaru",
            "email": "jplugaru@hotmail.fr",
            "homepage": "https://github.com/Gta-Cool"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.0,<3.0-dev",
        "components/inuit.css": "5.0.*"
    },
    "autoload": {
        "psr-0": { "GtaCool\\Bundle\\InuitCssBundle": "" }
    },
    "target-dir": "GtaCool/Bundle/InuitCssBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}